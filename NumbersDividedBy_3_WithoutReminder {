package oprajneniq;

import java.util.Scanner;

public class NumbersDividedBy_3_WithoutReminder {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);



        for (int i = 1; i <= 100; i += 3) {
            if (i /3==0) {
                i++;


            }
            System.out.println(i);

            }



        }

    }


